            <li><a class="special-menu" href="/logout">Logout</a>
            </li>
            <div class="container">
              <div class="row">
                    <div class="profile-header-container">
                      <div class="profile-header-img">
                            <img class="img-square" src="<%= user.photoAddress %>" />
                      </div>
                    </div>
              </div>
            </div>
            <% } else { %>
            <li><a href="/login">Log In</a>
