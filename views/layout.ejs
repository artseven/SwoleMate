    <%- body %>

<footer>
  <div id="footer">
    <div class="container">
      <div class="row">
        <div class="col-md-3">
          <h3 class="section-title">About Us</h3>
            <p>Stay in touch with the people who want you to become better version of yourself</p>
        </div>
        <div class="col-md-3">
					<h3 class="section-title">Our Address</h3>
						<p>120 SW 8th St, Miami, FL 33130</p>
						<p>+ 305 235 5698</p>
						<p><a href="#">techsfriend.com</a></p>
				</div>
        <div class="col-md-3">
          <h3 class="section-title"> Latest activity</h3>
        </div>
        <div class="col-md-3 animate-box">
					<h3 class="section-title">Send us your opinion</h3>
					<form class="contact-form">
						<div class="form-group">
							<label for="name" class="sr-only">Name</label>
							<input type="name" class="form-control" id="name" placeholder="Name">
						</div>
						<div class="form-group">
							<label for="email" class="sr-only">Email</label>
							<input type="email" class="form-control" id="email" placeholder="Email">
						</div>
						<div class="form-group">
							<label for="message" class="sr-only">Message</label>
							<textarea class="form-control" id="message" rows="7" placeholder="Message"></textarea>
						</div>
						<div class="form-group">
							<input type="submit" id="btn-submit" class="btn btn-send-message btn-md" value="Send Message">
						</div>
					</form>
				</div>

			<div class="row copy-right">
				<div class="col-md-6 col-md-offset-3 text-center">
					<p class="social-icons">
						<a href="#"><img class="social-icons grow" src="../images/icons/393-google.png"></a>
            <a href="#"><img class="social-icons grow" src="../images/icons/403-instagram.png"></a>
						<a href="#"><img class="social-icons grow" src="../images/icons/401-facebook.png"></a>
						<!-- <a href="#"><img class="social-icons" src="../images/icons/414-youtube.png"></a> -->
					</p>
					<p>Copyright 2017© Made by ArtSevenDesigns™<p> All Rights Reserved.</p>
				</div>
			</div>
		</div>
	</div>
</footer>
</body>
