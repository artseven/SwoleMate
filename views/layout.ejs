  <body>
  <header>
    <nav class="navbar navbar-inverse">
      <div class="container-fluid">
        <div class="navbar-header">
        </div>
        <img style="width: 5%; height: auto; margin:5px 25px 5px;" src="..\images\icons\89034aad8c2baac3b25a59af15d9721c.jpg">
        <div id="my-nav-bar">
          <ul class="nav navbar-nav" style="padding-top: 10px; padding-bottom: 0px;">

            <%  if (typeof user !== 'undefined') { %>
            <li><a href="/">Home</a>
            </li>
            <li><a href="/map">Find People</a>
            </li>
            <li><a href="/workout/new">New Workout</a>
            </li>
